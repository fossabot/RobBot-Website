{
  "plugins": [
    "@babel/proposal-class-properties",
    [
      "import",
      { "libraryName": "antd", "libraryDirectory": "lib", "style": "css" },
      "ant"
    ]
  ],
  "presets": [
    "@babel/preset-typescript",
    "@babel/preset-react",
    [
      "@babel/preset-env",
      {
        "modules": false,
        "targets": {
          "browsers": "> 1%, not dead",
          "node": "current"
        }
      }
    ]
  ]
}
